solid platef
    facet normal 1 0 0
        outer loop
            vertex 1 0 0
            vertex 1 0 0.6
            vertex 1 0.6 0.6
        endloop
    endfacet
facet normal 1 0 0
        outer loop
            vertex 1 0.6 0.6
            vertex 1 0 0.6
            vertex 1 0 0
        endloop
    endfacet
endsolid platef













set tics font "Times-Roman, 50" 
set xlabel font "Times-Roman, 50"
set ylabel font "Times-Roman, 50"
set border linewidth 3
set key font "Times-Roman, 50"
set key spacing 1
do for [i=1:7] {
    set linetype i lw 6
}

set title "Monitoring return temperature"
set ylabel 'Temperature (K)'
set xlabel 'Iteration'
plot "< cat log.mpirun | grep 'rack_inlet_outlet_temp on rack1B4CBD4A994E6797F70F5EF1F5863DB2_inlet' | cut -d'=' -f2" title 'Rack inlet Temp' with lines,\
     "< cat log.mpirun | grep 'AHU_supply_return_temp on acuF9CB05DA6465C302727AC2126F3ADC50_inlet' | cut -d'=' -f2" title 'AHU return Temp' with lines
      
pause 1
reread 

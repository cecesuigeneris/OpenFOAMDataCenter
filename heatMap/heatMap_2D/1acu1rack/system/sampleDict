FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat raw;
surfaceFormat raw;
interpolationScheme cellPoint;

fields
(
    T
    U
);

surfaces
(
    lowSlice
    {
        type            plane;
        basePoint       (0 0 1.8);
        normalVector    (0 0 1);
        interpolate	true;
    }

    underfloorSlice
    {
        type            plane;
        basePoint       (0 0 0.4);
        normalVector    (0 0 1);
        interpolate	true;
    }
);


